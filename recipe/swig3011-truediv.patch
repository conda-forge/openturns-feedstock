+#endif
 
 } // SquareComplexMatrix
 } // OT
diff --git a/python/src/SquareMatrix.i b/python/src/SquareMatrix.i
index 1f1ca41..e1cf0f1 100644
--- a/python/src/SquareMatrix.i
+++ b/python/src/SquareMatrix.i
