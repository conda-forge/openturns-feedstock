--- a/python/src/SquareComplexMatrix.i
+++ b/python/src/SquareComplexMatrix.i
@@ -20,7 +20,9 @@ namespace OT {
 
   OTComplexMatrixGetAccessors()
 
+#if SWIG_VERSION < 0x030011
